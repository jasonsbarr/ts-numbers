{
  "name": "@jasonsbarr/ts-numbers",
  "version": "1.0.0",
  "type": "module",
  "description": "A TypeScript implementation of Scheme's numeric tower",
  "publishConfig": {
    "access": "public"
  },
  "source": "./src/index.ts",
  "exports": {
    "require": "./dist/cjs/index.cjs",
    "default": "./dist/modern/index.js"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "unpkg": "./dist/umd/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier -w ./src",
    "build": "microbundle",
    "watch": "microbundle watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jasonsbarr/ts-numbers.git"
  },
  "keywords": [
    "numbers",
    "typescript",
    "scheme"
  ],
  "author": "Jason Barr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasonsbarr/ts-numbers/issues"
  },
  "homepage": "https://github.com/jasonsbarr/ts-numbers#readme",
  "devDependencies": {
    "microbundle": "^0.15.1",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1"
  }
}
